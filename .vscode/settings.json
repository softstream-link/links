{
    "rust-analyzer.cargo.features": [
        "full"
    ],
    "workbench.editor.revealIfOpen": true,
    "cSpell.words": [
        "atexit",
        "builtins",
        "busywait",
        "byteserde",
        "canonicalize",
        "chrono",
        "clbk",
        "clippy",
        "cltpool",
        "clts",
        "cogapp",
        "conid",
        "craits",
        "Decrementor",
        "devcontainer",
        "devcontainers",
        "devnull",
        "doctest",
        "dunder",
        "dunders",
        "dylib",
        "endianess",
        "eventstore",
        "getattr",
        "hbeat",
        "hbeats",
        "Initiatr",
        "isinstance",
        "kwargs",
        "levelname",
        "libpython",
        "maturin",
        "mdbook",
        "micromamba",
        "msger",
        "nanos",
        "nextest",
        "nonblocking",
        "popd",
        "println",
        "pushd",
        "pyclass",
        "pyfunction",
        "pymethods",
        "pymodule",
        "pytest",
        "Recver",
        "recvers",
        "repr",
        "rpath",
        "rustc",
        "RUSTDOCFLAGS",
        "rustfmt",
        "rustup",
        "serde",
        "setattr",
        "SOUPBIN",
        "soupbintcp",
        "struct",
        "successfull",
        "svcpool",
        "svcs",
        "traceback",
        "tshark",
        "Uncategorized",
        "Uninit",
        "unpark",
        "unsequenced",
        "ures",
        "usize",
        "workdir",
        "wouldblock"
    ],
    "python.analysis.typeCheckingMode": "basic",
    "python.analysis.autoImportCompletions": true,
    "python.terminal.activateEnvironment": false,
    "black-formatter.args": [
        "--line-length",
        "150"
    ],
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true,
    },
    "python.testing.pytestArgs": [
        "bindings/python/tests"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
}